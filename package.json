{
  "name": "me-architect-my-life",
  "version": "1.0.0",
  "description": "Personal Goal & Habit Tracker Desktop Application built with Electron",
  "homepage": "https://github.com/StephenBooysen/me-architect-my-life#readme",
  "bugs": {
    "url": "https://github.com/StephenBooysen/me-architect-my-life/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephenBooysen/me-architect-my-life.git"
  },
  "license": "ISC",
  "author": "Stephen Booysen",
  "type": "commonjs",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:3000 && electron . --dev\"",
    "dev:renderer": "vite",
    "dev:web": "concurrently \"npm run dev:renderer\" \"npm run dev:server\"",
    "dev:server": "node src/web/server.js",
    "start:web": "npm run build:renderer && node src/web/server.js",
    "build": "vite build && electron-builder",
    "build:renderer": "vite build",
    "dist": "vite build && electron-builder --publish=never",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "chart.js": "^4.2.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "electron": "^32.0.0",
    "express": "^4.21.2",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-markdown-editor-lite": "^1.3.4",
    "react-router-dom": "^6.8.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^7.6.0",
    "electron-builder": "^24.0.0",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "vite": "^4.3.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.stephenbooysen.architect-my-life",
    "productName": "Architect My Life",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/",
      "assets/",
      "node_modules/"
    ]
  }
}
